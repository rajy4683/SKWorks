# keyframing options
-keyframe_mode=AUTO
-dist_threshold=0.6
-inlier_threshold=0.7
-frame_dist_threshold=0.03

# connection options
-connection_mode=LASTN
-max_back_connections=2

# tracking options
-tracking_mode=LAST
-tracking_huber_delta=0.03
-tracking_dist_threshold=3.5
-tracking_error_threshold=0.5

# init/visualize
-init_type=ONEFRAME
#-vis_mode=LAST
-vis_mode=ALL

# mapping parameters
-pose_prior=0.1
-code_prior=1
-partial_relin_check=true
-relinearize_threshold=0.03
